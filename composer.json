{
    "name": "rnsrk/wisski_recipe_base_dev",
    "description": "Installs and configures the WissKI base environment (development branch) with mandatory modules.",
    "keywords": ["drupal recipe", "wisski"],
    "type": "drupal-recipe",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Robert Nasarek",
            "role": "Maintainer"
        }
    ],
    "repositories": {
        "cure53/dompurify": {
            "type": "package",
            "package": 
            {
                "name": "cure53/dompurify",
                "version": "3.1.6",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/cure53/DOMPurify/archive/3.1.6.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/cure53/DOMPurify",
                    "type": "git",
                    "reference": "main"
                },
                "extra": {
                    "installer-name": "dompurify",
                    "installer-paths": {
                        "webroot/libraries/dompurify": ["dist/*"]
                    }
                }
            }
        },
        "jackmoore/colorbox": {
            "type": "package",
            "package": 
            {
                "name": "jackmoore/colorbox",
                "version": "1.6.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/jackmoore/colorbox",
                    "type": "git",
                    "reference": "main"
                },
                "extra": {
                    "installer-name": "colorbox",
                    "installer-paths": {
                        "webroot/libraries/colorbox": ["dist/*"]
                    }
                }
            }
            
        },
        "rnsrk/wisski-mirador-integration":{
            "type": "package",
            "package": 
            {
                "name": "rnsrk/wisski-mirador-integration",
                "version": "1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/rnsrk/wisski-mirador-integration/archive/1.1.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/rnsrk/wisski-mirador-integration",
                    "type": "git",
                    "reference": "main"
                },
                "extra": {
                    "installer-name": "wisski-mirador-integration",
                    "installer-paths": {
                        "webroot/libraries/wisski-mirador-integration": ["dist/*"]
                    }
                }
            }
            
        },
        "ruven/iipmooviewer": {
            "type": "package",
            "package": 
            {
                "name": "ruven/iipmooviewer",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/ruven/iipmooviewer/archive/refs/heads/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/ruven/iipmooviewer",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "installer-name": "iipmooviewer",
                    "installer-paths": {
                        "webroot/libraries/iipmooviewer": ["dist/*"]
                    }
                }
            }
        }
    },
    "require": {
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/wisski": "3.x-dev@dev"
    }
}